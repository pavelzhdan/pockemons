{
  "compilerOptions": {
    "outDir": "./docs/",
    "sourceMap": true,
    "esModuleInterop": true,
    "noImplicitAny": true,
    "module": "esnext",
    "target": "es5",
    "strict": true,
    "jsx": "react",
    "allowJs": true,
    "skipLibCheck": true,
    "newLine": "LF",
    "moduleResolution": "node",
    "types": ["node"]
  }
}
